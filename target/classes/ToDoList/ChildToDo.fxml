<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MyAppControllers.ChildToDoListLoginController">
   <children>
      <Label layoutX="25.0" layoutY="53.0" prefHeight="17.0" prefWidth="143.0" text="Enter your child's name:">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="Name" layoutX="22.0" layoutY="89.0" prefHeight="25.0" prefWidth="258.0" promptText="child name" />
      <Button fx:id="submit" layoutX="133.0" layoutY="153.0" mnemonicParsing="false" onAction="#VerifyChildLogin" text="Submit">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </Button>
      <Label fx:id="childuser" layoutX="47.0" layoutY="267.0" prefHeight="17.0" prefWidth="224.0" />
      <Button fx:id="ChildUserGoBack" layoutX="271.0" layoutY="352.0" mnemonicParsing="false" onAction="#ChildLoginUserGoBack" text="Back" />
   </children>
</AnchorPane>
