<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="358.0" prefWidth="305.0" stylesheets="@DecorateHealth.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MyAppControllers.BMIController">
    <children>
        <TitledPane animated="false" layoutY="-5.0" prefHeight="25.0" prefWidth="309.0" text="Count Your BMI" textFill="#c76fa2">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
            <font>
                <Font size="16.0" />
            </font>
        </TitledPane>
        <TextField fx:id="BMI" layoutX="19.0" layoutY="179.0" prefHeight="26.0" prefWidth="178.0" text="Your BMI :">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></TextField>
        <TextField fx:id="HealthyWeight" layoutX="19.0" layoutY="231.0" prefHeight="26.0" prefWidth="236.0" text="Your Healthy Weight :">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></TextField>
        <TextField fx:id="Suggestion" layoutX="19.0" layoutY="281.0" prefHeight="26.0" prefWidth="284.0" text="Suggestion:">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></TextField>
      <TextField fx:id="Weight" layoutX="59.0" layoutY="47.0" promptText="Add Weight in Kilogram">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></TextField>
      <TextField fx:id="Height" layoutX="59.0" layoutY="88.0" promptText="Add Height in Inches">
         <font>
            <Font name="Comic Sans MS" size="11.0" />
         </font></TextField>
      <Button fx:id="Submit" layoutX="93.0" layoutY="129.0" mnemonicParsing="false" onAction="#CalculateBMI" text="Submit" textFill="#c76fa2">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></Button>
      <Label fx:id="validInput" layoutX="169.0" layoutY="133.0" prefHeight="18.0" prefWidth="120.0" textFill="#d92727" />
      <ImageView fitHeight="59.0" fitWidth="57.0" layoutX="2.0" layoutY="33.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@wghtMach-removebg-preview.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="68.0" fitWidth="87.0" layoutX="208.0" layoutY="74.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@heightMach-removebg-preview.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="49.0" fitWidth="92.0" layoutX="201.0" layoutY="169.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@bmi-removebg-preview.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="57.0" fitWidth="54.0" layoutX="255.0" layoutY="230.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@healthwgt-removebg-preview.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="57.0" fitWidth="99.0" layoutX="2.0" layoutY="301.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@suggest-removebg-preview.png" />
         </image>
      </ImageView>
      <Button fx:id="HealthGoBack" layoutX="247.0" layoutY="320.0" mnemonicParsing="false" onAction="#HealthTrackerGoBack" text="Back" />
    </children>
</AnchorPane>
